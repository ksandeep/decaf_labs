class List {
	private List next;
	public int length(){
		if (this == null) 
			return 0;
		else
			return 1 + next.length();
	}
}

class IntList extends List {
	private int element;
	public int get_element() {
		return this.element;
	}
	public int sum() {
		if (this == null)
			return 0;
		else
			return this.get_element()  + next.sum();
	}
}
